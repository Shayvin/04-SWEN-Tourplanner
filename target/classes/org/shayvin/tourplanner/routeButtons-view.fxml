<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.shayvin.tourplanner.view.RouteButtonsView">

    <Label text="%addRouteTextRoute"/>
    <Button fx:id="addButton" onAction="#onAdd" mnemonicParsing="false" prefHeight="25.0" prefWidth="52.0" HBox.hgrow="ALWAYS" text="%addRouteTextAdd"/>
    <Button fx:id="removeButton" onAction="#onRemove" mnemonicParsing="false" prefHeight="25.0" prefWidth="75.0" HBox.hgrow="ALWAYS" text="%addRouteTextRemove"/>
    <Button fx:id="editButton" onAction="#onEdit" mnemonicParsing="false" prefHeight="25.0" prefWidth="52.0" HBox.hgrow="ALWAYS" text="%addRouteTextEdit"/>
    <Button fx:id="saveButton" onAction="#onSave" mnemonicParsing="false" prefHeight="25.0" prefWidth="52.0" HBox.hgrow="ALWAYS" text="%addRouteTextSave"/>

</HBox>
