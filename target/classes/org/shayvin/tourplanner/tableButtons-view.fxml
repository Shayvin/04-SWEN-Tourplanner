<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.shayvin.tourplanner.view.TableButtonView">

    <Label text="Tour Logs" />
    <Button mnemonicParsing="false" text="%addTourButtonTextAdd" fx:id="tourLogAddButton" onAction="#onAdd"/>
    <Button mnemonicParsing="false" text="%addTourButtonTextDelete" fx:id="tourLogDeleteButton" onAction="#onDelete"/>
    <Button mnemonicParsing="false" text="%addTourButtonTextEdit" fx:id="tourLogEditButton" onAction="#onEdit"/>
</HBox>
